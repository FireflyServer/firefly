{
	"version": "1.0.0-beta2",
	"compilationOptions": {
		"warningsAsErrors": true
	},
	"configurations": {
		"Debug": {
			"compilationOptions": {
				"define": [ "DEBUG", "TRACE" ]
			}
		},
		"Release": {
			"compilationOptions": {
				"define": [ "RELEASE", "TRACE" ],
				"optimize": true
			}
		}
	},
    "dependencies": {
        "Microsoft.AspNet.Hosting": "1.0.0-beta2"
    },
    "frameworks": {
        "aspnetcore50": {
            "dependencies": {
                "System.Net.Sockets": "4.0.0.0",
                "System.Diagnostics.Tracing": "4.0.10.0",
                "System.Diagnostics.TextWriterTraceListener": "4.0.0.0",
                "System.Diagnostics.TraceSource": "4.0.0.0",
                "System.Threading.ThreadPool": "4.0.10.0",
                "System.Text.Encoding": "4.0.10.0-beta-*"
            }
        },
        "aspnet50": { }
    }
}
